<templateSet group="lvComments">
  <template name="*" value="*&#10; * ${tags}&#10; * @Copyright (c) 2012,北京吉威时代软件股份有限公司.All Rights Reserved.&#10; * @link http://www.geoway.com.cn&#10; * @author 吕晓飞&#10; * @since $date$ $time$&#10; * @version 1.0&#10; */" shortcut="ENTER" description="普通注释" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="**" value="**&#10; * @Description: $discription$&#10; * @Param: $params$&#10; * @return: $return$&#10; * @since: $date$ $time$&#10; */" shortcut="ENTER" description="方法注释" toReformat="false" toShortenFQNames="true">
    <variable name="discription" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="params" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>